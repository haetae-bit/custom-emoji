					"required": [
						"image",
						"alt"
					]
				},
				"shortcode": {
					"type": "string",
					"required": true
				},
				"description": {
					"type": "string"
				},
				"fallback": {
					"type": "string",
					"required": true
				}
			},
			"required": [
				"image",
				"shortcode",
				"fallback"
			]
		}
	}
